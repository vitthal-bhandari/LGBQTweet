    "# Compute the Cohen's kappa score between the 'human_annotation' and 'gpt_annotation' columns\n",
    "kappa = cohen_kappa_score(merged_df['human_annotation'], merged_df['gpt_annotation'], labels=['non-hateful', 'hateful'])\n",
